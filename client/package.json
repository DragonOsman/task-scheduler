{
  "name": "task-scheduler",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ramonak/react-progress-bar": "^5.0.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.40",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "bootstrap": "^5.3.1",
    "eslint-plugin-html": "^7.1.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "howler": "^2.2.3",
    "passport": "^0.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-timer-hook": "^3.0.7",
    "typescript": "^4.9.5",
    "vite": "^4.4.9",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "react-app",
      "plugin:react/recommended"
    ],
    "plugins": [
      "html",
      "react"
    ],
    "rules": {
      "prefer-const": "error",
      "yoda": "error",
      "space-before-function-paren": "off",
      "no-var": "warn",
      "quotes": [
        "warn",
        "double",
        {
          "allowTemplateLiterals": true
        }
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "semi": "error",
      "no-undef": "warn"
    },
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": "latest"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/howler": "^2.2.8"
  }
}
